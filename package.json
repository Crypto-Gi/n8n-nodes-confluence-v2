{
  "name": "n8n-nodes-confluence-v2",
  "version": "0.1.0",
  "description": "n8n community node for Confluence API v2 with hierarchical page fetching",
  "license": "MIT",
  "author": "Crypto-Gi",
  "main": "dist/nodes/ConfluenceV2/ConfluenceV2.node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Crypto-Gi/n8n-nodes-confluence-v2.git"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "confluence",
    "atlassian",
    "api-v2",
    "workflow",
    "automation"
  ],
  "homepage": "https://github.com/Crypto-Gi/n8n-nodes-confluence-v2#readme",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write nodes credentials",
    "lint": "eslint nodes credentials --ext .js,.ts",
    "lint:fix": "eslint nodes credentials --ext .js,.ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ConfluenceV2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ConfluenceV2/ConfluenceV2.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^16.18.0",
    "@types/request-promise-native": "^1.0.17",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.9.5"
  }
}
